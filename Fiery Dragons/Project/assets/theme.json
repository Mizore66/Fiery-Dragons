{
  "button": {
    "colours": {
      "normal_border": "#33251D",
      "normal_bg": "#84624B",
      "normal_text": "White"
    },
    "misc": {
      "shape": "rounded_rectangle",
      "shape_corner_radius": "10",
      "border_width": "2"
    },
    "font": {
      "size": "18",
      "name": "Calist",
      "regular_path": "assets/fonts/CALIST.ttf"
    }
  },
  "text_box": {
    "colours": {
      "dark_bg": "#2F261F",
      "normal_border": "#999999",
      "link_text": "#FF0000",
      "link_hover": "#FFFF00",
      "link_selected": "#FFFFFF"
    },
    "misc": {
      "border_width": "1",
      "padding": "10,10",
      "link_normal_underline": "0",
      "link_hover_underline": "1",
      "line_spacing": "1.0"
    }
  },
  "text_box.vertical_scroll_bar": {
    "colours": {
      "dark_bg": "#505068"
    }
  },
  "text_box.vertical_scroll_bar.#sliding_button": {
    "misc": {
      "border_width": "1"
    }
  },
  "drop_down_menu": {
    "misc": {
      "expand_direction": "down",
      "shape": "rounded_rectangle",
      "shape_corner_radius": "10",
      "border_width": "1"
    },
    "colours": {
      "normal_border": "#33251D",
      "normal_bg": "#25292e",
      "hovered_bg": "#35393e"
    }
  },
  "drop_down_menu.#selected_option": {
    "misc": {
      "border_width": "1",
      "open_button_width": "10",
      "shape": "rounded_rectangle",
      "shape_corner_radius": "10"
    }
  },
  "drop_down_menu.#expand_button": {
    "misc": {
      "border_width": "1",
      "shape": "rounded_rectangle",
      "shape_corner_radius": "10"
    }
  },
  "drop_down_menu.#drop_down_options_list": {
    "misc": {
      "border_width": "1",
      "shape": "rounded_rectangle",
      "shape_corner_radius": "5"
    }
  },
  "drop_down_menu.#drop_down_options_list.button": {
    "misc": {
      "border_width": "1",
      "shape": "rounded_rectangle",
      "shape_corner_radius": "5"
    }
  },
  "window":
   {
      "colours":
      {
         "dark_bg":"#32271C",
         "normal_border": "#999999"
      },

      "misc":
      {
         "shape": "rounded_rectangle",
         "shape_corner_radius": "10",
         "border_width": "1",
         "shadow_width": "15",
         "title_bar_height": "40"
      }
   },
   "window.#title_bar":
   {
      "misc":
      {
         "text_horiz_alignment": "center"
      }
   }
}